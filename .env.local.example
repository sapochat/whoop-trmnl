# WHOOP API Credentials
# Create these at https://developer.whoop.com
WHOOP_CLIENT_ID=your_client_id_here
WHOOP_CLIENT_SECRET=your_client_secret_here

# Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Vercel KV (for token storage)
KV_URL=your_kv_url_here
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here
KV_REST_API_READ_ONLY_TOKEN=your_kv_rest_api_read_only_token_here

# TRMNL Configuration
# This is the URL that TRMNL will use to poll your plugin
NEXT_PUBLIC_TRMNL_PLUGIN_URL=https://your-vercel-app.vercel.app/api/dashboard
